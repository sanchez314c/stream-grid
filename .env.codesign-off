# CODE SIGNING DISABLED CONFIGURATION
# This configuration completely disables all code signing for StreamGRID builds

# Electron Builder Code Signing Override
CSC_IDENTITY_AUTO_DISCOVERY=false
CSC_LINK=
CSC_KEY_PASSWORD=
CSC_NAME=
CSC_PROVIDER=

# macOS Code Signing Disable
MAC_CODE_SIGNING=false
ELECTRON_BUILDER_CODE_SIGNING=false
HARDENED_RUNTIME=false
GATEKEEPER_ASSESS=false

# Windows Code Signing Disable
WINDOWS_CODE_SIGNING=false
CSC_WINDOWS_SIGN=false

# Linux Code Signing Disable (not typically used but disabled for completeness)
LINUX_CODE_SIGNING=false

# General Build Signing Flags
SIGN_APP=false
SIGN_BUILD=false
BUILD_CODE_SIGNING=false
ELECTRON_BUILDER_SIGN=false

# Build Configuration Overrides
BUILD_ARGS="--publish=never --mac.identity=null --win.certificateFile='' --linux.sign=false"
ELECTRON_BUILDER_ARGS="--publish=never --mac.identity=null --win.certificateFile='' --linux.sign=false"

# Additional flags to prevent any code signing attempts
CSC_FOR_PULL_REQUEST=false
SKIP_NOTARIZATION=true
SKIP_CODESIGN=true

# Force disable all signing attempts
FORCE_NO_CODESIGN=true
DISABLE_CODESIGNING=true